{{!-- Loop over each object in the posts array --}}
<ul class="post-list">
    {{#each posts as |post|}}
    <li>
        {{> post-info post}}
        <p>Created by {{user.name}} on {{format_date this.date_created}}</p>
        <button class="edit-btn"> Edit </button>
        <button class="delete-btn"> Delete </button>
    </li>
    {{/each}}
</ul>